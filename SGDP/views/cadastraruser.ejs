
<div class="div_table_login " onload="esconderElemento()">  
  
  <% if (message !="") { %>
    <section id="meuElemento" class=" <%= type  %> close">
      <p><%= message  %></p>
    </section>
  <% } %>

    <link rel="stylesheet" href="css/style.css">
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-QWTKZyjpPEjISv5WaRU9OFeRpok6YctnYmDr5pNlyT2bRjXh0JMhjY6hW+ALEwIH" crossorigin="anonymous">
  <form method="post"  action="/novousuario">
      <div class="mb-3">
        <label for="exampleInputEmail1" class="form-label">Nome completo:</label>
        <input name="nome" type="text" class="form-control" id="exampleInputEmail1" placeholder="Nome" aria-describedby="emailHelp">
      </div>
      
      <div class="mb-3">
        <label for="exampleInputPassword1" class="form-label">CPF</label>
        <input name="cpf" type="password" class="form-control" placeholder="CPF" id="exampleInputPassword1">
      </div>
      <div class="mb-3">
        <label for="exampleInputPassword1" class="form-label">Unidade</label>
        <input name="unidade" type="text" class="form-control" placeholder="Unidade" id="exampleInputPassword2">
      </div>
      <div class="mb-3 display_none">
        <label for="exampleInputPassword1" class="form-label ">Age</label>
        <input name="age" readonly  value="1" type="password" class="form-control" placeholder="Unidade" id="exampleInputPassword3">
      </div>
      <div class="mb-3 display_none">
        <label for="exampleInputPassword1" class="form-label">Type</label>
        <input name="type" readonly value="DIG" type="password" class="form-control" placeholder="Unidade" id="exampleInputPassword4">
      </div>
      
      <div class="mb-3">
        <label for="exampleInputPassword1" class="form-label">Password</label>
        <input name="senha" type="password" class="form-control" placeholder="Senha" id="exampleInputPassword5">
      </div>
      
      <button type="submit" class="btn btn-primary">Submit</button>
      <a href="/" class="alink_cads btn btn-danger">Cancelar</a>
      
    </form>
    <script src="js/scriptis.js"></script>
    
  </div>